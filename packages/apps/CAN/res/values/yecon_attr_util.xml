<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Compass">
        <attr name="needle" format="reference" />
        <attr name="clock_bg" format="reference" />
    </declare-styleable>
    <declare-styleable name="FuelSeekBar">
        <attr name="can_move" format="boolean" />
    </declare-styleable>
    <declare-styleable name="WindLevel">

        <!-- 最大风量 -->
        <attr name="MaxLevel" format="integer" />
        <!-- 普通状态 -->
        <attr name="BmpNormal" format="reference" />
        <!-- 高亮状态 -->
        <attr name="BmpSelect" format="reference" />
        <!-- 每隔之间的间距 -->
        <attr name="Spacing" format="integer" />
    </declare-styleable>

</resources>
