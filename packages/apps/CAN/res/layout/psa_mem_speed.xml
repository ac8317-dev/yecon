<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/can_bg"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text="@string/str_psa_mem_speed"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />
        <CheckBox 
            android:id="@+id/psa_btn_open_mem_speed"
            android:layout_marginLeft="@dimen/dps_psa_speed_space"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/checkbox_on_off" 
            android:button="@null" 
            android:layout_gravity="center_vertical"/>

        <TextView
            android:id="@+id/psa_btn_reset_mem_speed"
            android:layout_marginLeft="@dimen/dps_psa_speed_space"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@drawable/selector_fuel_del"
            android:gravity="center"
            android:text="@string/str_psa_reset"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="@dimen/dps_psa_speed_width"
            android:layout_height="wrap_content"
            android:text="@string/str_psa_speed1"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <TextView
            android:id="@+id/psa_tv_mem_speed_value1"
            android:layout_width="@dimen/dps_psa_speed_value_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <SeekBar
            android:id="@+id/psa_skbar_speed1"
            android:layout_width="@dimen/compass_skbar_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:progressDrawable="@drawable/compass_seekbar_img"
            android:thumb="@drawable/compass_thumb" />

        <CheckBox
            android:id="@+id/psa_box_speed1"
            android:layout_marginLeft="@dimen/dps_psa_speed_space"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            android:layout_gravity="center_vertical"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="@dimen/dps_psa_speed_width"
            android:layout_height="wrap_content"
            android:text="@string/str_psa_speed2"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <TextView
            android:id="@+id/psa_tv_mem_speed_value2"
            android:layout_width="@dimen/dps_psa_speed_value_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <SeekBar
            android:id="@+id/psa_skbar_speed2"
            android:layout_width="@dimen/compass_skbar_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:progressDrawable="@drawable/compass_seekbar_img"
            android:thumb="@drawable/compass_thumb" />

        <CheckBox
            android:id="@+id/psa_box_speed2"
            android:layout_marginLeft="@dimen/dps_psa_speed_space"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            android:layout_gravity="center_vertical"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="@dimen/dps_psa_speed_width"
            android:layout_height="wrap_content"
            android:text="@string/str_psa_speed3"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <TextView
            android:id="@+id/psa_tv_mem_speed_value3"
            android:layout_width="@dimen/dps_psa_speed_value_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <SeekBar
            android:id="@+id/psa_skbar_speed3"
            android:layout_width="@dimen/compass_skbar_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:progressDrawable="@drawable/compass_seekbar_img"
            android:thumb="@drawable/compass_thumb" />

        <CheckBox
            android:id="@+id/psa_box_speed3"
            android:layout_marginLeft="@dimen/dps_psa_speed_space"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            android:layout_gravity="center_vertical"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="@dimen/dps_psa_speed_width"
            android:layout_height="wrap_content"
            android:text="@string/str_psa_speed4"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <TextView
            android:id="@+id/psa_tv_mem_speed_value4"
            android:layout_width="@dimen/dps_psa_speed_value_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <SeekBar
            android:id="@+id/psa_skbar_speed4"
            android:layout_width="@dimen/compass_skbar_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:progressDrawable="@drawable/compass_seekbar_img"
            android:thumb="@drawable/compass_thumb" />

        <CheckBox
            android:id="@+id/psa_box_speed4"
            android:layout_marginLeft="@dimen/dps_psa_speed_space"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            android:layout_gravity="center_vertical"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="@dimen/dps_psa_speed_width"
            android:layout_height="wrap_content"
            android:text="@string/str_psa_speed5"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <TextView
            android:id="@+id/psa_tv_mem_speed_value5"
            android:layout_width="@dimen/dps_psa_speed_value_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:textColor="@color/selector_color_state"
            android:textSize="@dimen/textsize_general" />

        <SeekBar
            android:id="@+id/psa_skbar_speed5"
            android:layout_width="@dimen/compass_skbar_width"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:progressDrawable="@drawable/compass_seekbar_img"
            android:thumb="@drawable/compass_thumb" />

        <CheckBox
            android:id="@+id/psa_box_speed5"
            android:layout_marginLeft="@dimen/dps_psa_speed_space"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            android:layout_gravity="center_vertical"/>
    </LinearLayout>

</LinearLayout>