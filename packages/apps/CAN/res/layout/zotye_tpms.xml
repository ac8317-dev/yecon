<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <ImageView
            android:id="@+id/dz7_tpms_imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:src="@drawable/toyota_tpms_bg" />

        <ImageView
            android:id="@+id/dz7_tpms_imageViewLF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/dz7_tpms_imageView1"
            android:layout_marginTop="55dp"
            android:layout_toLeftOf="@+id/dz7_tpms_imageView1"
            android:src="@drawable/toyota_tpms_trice" />

        <ImageView
            android:id="@+id/dz7_tpms_ImageViewRF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/dz7_tpms_imageViewLF"
            android:layout_toRightOf="@+id/dz7_tpms_imageView1"
            android:src="@drawable/toyota_tpms_trice" />

        <ImageView
            android:id="@+id/dz7_tpms_ImageViewLR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/dz7_tpms_ImageViewRR"
            android:layout_toLeftOf="@+id/dz7_tpms_imageView1"
            android:src="@drawable/toyota_tpms_trice" />

        <ImageView
            android:id="@+id/dz7_tpms_ImageViewRR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/dz7_tpms_imageView1"
            android:layout_marginBottom="43dp"
            android:layout_toRightOf="@+id/dz7_tpms_imageView1"
            android:src="@drawable/toyota_tpms_trice" />

        <TextView
            android:id="@+id/tx_dz7_tpms_lf_val"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/dz7_tpms_imageViewLF"
            android:layout_marginRight="@dimen/tx_curfuel_trip_padding"
            android:layout_toLeftOf="@+id/dz7_tpms_imageViewLF"
            android:textSize="@dimen/tx_default_size" />

        <TextView
            android:id="@+id/tx_dz7_tpms_rf_val"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/tx_dz7_tpms_lf_val"
            android:layout_alignTop="@+id/tx_dz7_tpms_lf_val"
            android:layout_marginLeft="@dimen/tx_curfuel_trip_padding"
            android:layout_toRightOf="@+id/dz7_tpms_ImageViewRF"
            android:textSize="@dimen/tx_default_size" />

        <TextView
            android:id="@+id/tx_dz7_tpms_lr_val"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/tx_dz7_tpms_lf_val"
            android:layout_alignTop="@+id/dz7_tpms_ImageViewLR"
            android:textSize="@dimen/tx_default_size" />

        <TextView
            android:id="@+id/tx_dz7_tpms_rr_val"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/tx_dz7_tpms_rf_val"
            android:layout_alignTop="@+id/dz7_tpms_ImageViewRR"
            android:textSize="@dimen/tx_default_size" />
    </RelativeLayout>
    

</RelativeLayout>
