<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >

    <TextView
        android:id="@+id/tv_textview1"
        style="@style/FreqNumTextViewTheme"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/tv_textview2"
        style="@style/FreqNumTextViewTheme"
        android:layout_toRightOf="@id/tv_textview1" />

    <TextView
        android:id="@+id/tv_textview3"
        style="@style/FreqNumTextViewTheme"
        android:layout_toRightOf="@id/tv_textview2" />

    <TextView
        android:id="@+id/tv_textview4"
        style="@style/FreqNumTextViewTheme"
        android:layout_toRightOf="@id/tv_textview3"
        android:text="@string/fmr_freq_num_point" />

    <TextView
        android:id="@+id/tv_textview5"
        style="@style/FreqNumTextViewTheme"
        android:layout_toRightOf="@id/tv_textview4" />

    <TextView
        android:id="@+id/tv_textview6"
        style="@style/FreqNumTextViewTheme"
        android:layout_toRightOf="@id/tv_textview5" />

</RelativeLayout>