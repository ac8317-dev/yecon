	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:layout_marginLeft="40dp"
	    android:layout_marginTop="40dp"
	    >
	    <LinearLayout 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="vertical"
	        >
	        <LinearLayout 
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        >
	        	<TextView 
	        	    android:id="@+id/btn_call_one"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_1"
	        	    style="@style/CallNumber"
	        	    />
	        	<TextView 
	        	    android:id="@+id/btn_call_two"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_2"
	        	    style="@style/CallNumber"
	        	    />
	        	<TextView 
	        	    android:id="@+id/btn_call_three"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_3"
	        	    style="@style/CallNumber"
	        	    />
	    	</LinearLayout>
	    	<LinearLayout 
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        >
	        	<TextView 
	        	    android:id="@+id/btn_call_four"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_4"
	        	    style="@style/CallNumber"
	        	    />
	        	<TextView 
	        	    android:id="@+id/btn_call_five"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_5"
	        	    style="@style/CallNumber"
	        	    />
	        	<TextView 
	        	    android:id="@+id/btn_call_six"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_6"
	        	    style="@style/CallNumber"
	        	    />
	    	</LinearLayout>
	    	<LinearLayout 
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        >
	        	<TextView 
	        	    android:id="@+id/btn_call_seven"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_7"
	        	    style="@style/CallNumber"
	        	    />
	        	<TextView 
	        	    android:id="@+id/btn_call_eight"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_8"
	        	    style="@style/CallNumber"
	        	    />
	        	<TextView
	        	    android:id="@+id/btn_call_nine"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_9"
	        	    style="@style/CallNumber"
	        	    />
	    	</LinearLayout>
	    	<LinearLayout 
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        >
	        	<TextView
	        	    android:id="@+id/btn_call_asterisk"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_star"
	        	    style="@style/CallNumber"
	        	    />
	        	<TextView
	        	    android:id="@+id/btn_call_zero"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_0"
	        	    style="@style/CallNumber"
	        	    />
	        	<TextView 
	        	    android:id="@+id/btn_call_pound"
	        	    android:layout_width="wrap_content"
	        	    android:layout_height="wrap_content"
	        	    android:text="@string/number_sign"
	        	    style="@style/CallNumber"
	        	    />
	    	</LinearLayout>
	    </LinearLayout>
	    
	    <LinearLayout 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="vertical"
	        android:layout_marginLeft="40dp"
	        >
	    	<LinearLayout 
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        android:layout_gravity="center_horizontal"
		        >
		        <TextView
				  	android:id="@+id/text_call_info"
				  	android:layout_width="wrap_content"
				  	android:layout_height="wrap_content"
				  	android:background="@drawable/text_call_info_bg"
				  	android:singleLine="true"
				  	android:typeface="normal"
				  	android:textSize="28sp"
				  	android:textColor="#ffffff"
				  	android:gravity="left|center_vertical"
				  	android:paddingLeft="4dp"
				  	android:paddingRight="4dp"
				 />
		        <ImageButton 
		            android:id="@+id/btn_del_num"
		            android:contentDescription="@null"
	  				android:background="@drawable/selector_del_number"
			  		android:layout_width="wrap_content"
			  		android:layout_height="wrap_content"
			  		/>
	        </LinearLayout>
	        <LinearLayout 
		        android:layout_width="match_parent"
		        android:layout_height="124dp"
		        android:orientation="vertical"
		        android:layout_gravity="center_horizontal"
		        >
		       
	        </LinearLayout>
	        <LinearLayout 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        android:layout_gravity="center_horizontal"
		        >
		       <TextView
		           android:id="@+id/btn_call"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:textColor="#fff"
		           android:textSize="32sp"
		           android:gravity="center"
		           android:background="@drawable/selector_call_btn_left"
		           android:text="@string/BtDialerLabel"
		           android:enabled="false"
		           />
		       <TextView
		           android:id="@+id/bluetooth_pairing"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:textColor="#fff"
		           android:textSize="32sp"
		           android:gravity="center"
		        	android:text="@string/call_bluetooth_pairing"
		           android:background="@drawable/selector_call_btn_right"
		           />
		       <TextView
		           android:id="@+id/btn_add"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:textColor="#fff"
		           android:textSize="28sp"
		           android:visibility="gone"
		           />
	        </LinearLayout> 
	    </LinearLayout>
	</LinearLayout>
	<FrameLayout 
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        >
		<LinearLayout 
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    android:layout_gravity="bottom"
		    >
	        <TextView
	            android:id="@+id/btn_call_back"
		        style="@style/BottomBarTextStyle"
		        android:text="@string/back"
		        />
		    <TextView 
		        android:id="@+id/call_phone_book"
		        style="@style/BottomBarTextStyle"
		        android:text="@string/BtPhonebookLabel"
		        />
		    <TextView 
		        android:id="@+id/call_history"
		        style="@style/BottomBarTextStyle"
		        android:text="@string/BtCallHistoryLabel"
		        />
		    <TextView 
		        android:id="@+id/call_bt_music"
		        style="@style/BottomBarTextStyle"
		        android:text="@string/BtMusicLabel"
		        />
		</LinearLayout>
	</FrameLayout>
</LinearLayout>